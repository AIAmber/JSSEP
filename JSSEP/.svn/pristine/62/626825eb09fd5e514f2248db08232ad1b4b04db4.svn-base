.nav-tabs{border-bottom: 0px solid #ddd;}
.nav-tabs>li.active>a, .nav-tabs>li.active>a:focus, .nav-tabs>li.active>a:hover{border: 0px solid #ddd;border-bottom: 3px solid #3C8DBC;}
.tab-content{padding:3px;padding-top:15px;background-color:#F8F9FA;}
.modal-footer{border-top: 0px solid #e5e5e5;}
.modal-body{padding-bottom: 0px;}
.nodeAdd-dialog .modal-dialog{width:50%;margin-top:10%}
.node-dialog .modal-dialog{width:10%;margin-top:20%}
.form-horizontal .form-group{margin-right: 0px;}
.form-group {margin-bottom: 10px;}
.form-control{height: 30px;}
#resetbtn{margin-right: 5px;}
#publishDate,select{cursor:pointer;}
.fa-toggle-on,.fa-toggle-off{color:#3bb54b;font-size: 18px;}
.form-control-feedback{height: 30px;line-height: 30px;}