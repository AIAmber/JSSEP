.statisticalChart-right{
	width:49%;
	float:right;
	height:50%;
	border-color: #f5f5f5;
}
.statisticalChart-left{
	width:49%;
	float:left;
	height:50%;
	border-color: #f5f5f5;
}
.statisticalChart,.statisticalChart .panel-body,.chartContext{
	height:100%;
}
.chartContext{
	height:100%;
}
.statisticalChart-right .panel-heading,.statisticalChart-left .panel-heading{
	border-bottom: 0px;
	background-color: #fbfbfa;
}
.panel{
    -webkit-box-shadow: 0 0px 0px rgba(0,0,0,.05)!important;
}